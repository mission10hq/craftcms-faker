{
    "name": "mission10/craftcms-faker",
    "description": "Fake entry fields for CraftCMS",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin"
    ],
    "require": {
        "craftcms/cms": "^4.0|^5.0",
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Mission10\\CraftcmsFaker\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mission10",
            "email": "cameron@mission10.co.uk"
        }
    ],
    "extra": {
        "name": "Faker",
        "handle": "faker",
        "class": "Mission10\\CraftcmsFaker\\Faker"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
